<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent_color"
    tools:context=".TriFollowUpActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvdatapostd"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="32dp"
        android:background="#01579B"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.recyclerview.widget.RecyclerView>

    <TextView
        android:id="@+id/tvfundbdx"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="EuroCreditF"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/tvfundacx"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvfundbx"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="ABS.F"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/tvfundax"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvreservec"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Reserve" />

    <TextView
        android:id="@+id/tvfundacx"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="WorldEquityIndex"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/tvfundbx"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461" />

    <TextView
        android:id="@+id/tvfundax"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="CoreEuro.GovIndexF"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#0B4158"
        android:fontFamily="@font/ubuntu_light"
        android:text="To"
        android:textAlignment="center"
        android:textColor="@color/data_text_color"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/tvreservec"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#1EA6C0"
        android:fontFamily="@font/ubuntu_light"
        android:text="Gr"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#5CBBC8"
        android:fontFamily="@font/ubuntu_light"
        android:text="%"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="34dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#0E5860"
        android:fontFamily="@font/ubuntu_light"
        android:text="Valu"
        android:textAlignment="center"
        android:textColor="#F1CC50"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostd"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>