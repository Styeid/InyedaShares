<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent_color"
    tools:context=".SecFollowUpActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvdatapostc"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="32dp"
        android:background="#01579B"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvreserveb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Reserve" />

    <TextView
        android:id="@+id/tvfundac"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="224dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="WorldEquityIndex"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/tvreserveb"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvfundbd"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="192dp"
        android:fontFamily="@font/ubuntu_light"
        android:text="EuroCreditF"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView35"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#0B4158"
        android:fontFamily="@font/ubuntu_light"
        android:text="To"
        android:textAlignment="center"
        android:textColor="@color/data_text_color"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/tvfundac"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_marginStart="28dp"
        android:background="#1EA6C0"
        android:fontFamily="@font/ubuntu_light"
        android:text="X/"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView31"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#5CBBC8"
        android:fontFamily="@font/ubuntu_light"
        android:text="PL"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView32"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#1EA6C0"
        android:fontFamily="@font/ubuntu_light"
        android:text="%*"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView33"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
