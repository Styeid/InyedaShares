        android:background="#5CBBC8"
        android:fontFamily="@font/ubuntu_light"
        android:text="%"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#0B4158"
        android:fontFamily="@font/ubuntu_light"
        android:text="To"
        android:textAlignment="center"
        android:textColor="@color/data_text_color"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/tvfundbd"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#1EA6C0"
        android:fontFamily="@font/ubuntu_light"
        android:text="X/"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView40"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#5CBBC8"
        android:fontFamily="@font/ubuntu_light"
        android:text="PL"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView36"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:background="#1EA6C0"
        android:fontFamily="@font/ubuntu_light"
        android:text="%*"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintStart_toEndOf="@+id/textView38"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_marginEnd="16dp"
        android:background="#5CBBC8"
        android:fontFamily="@font/ubuntu_light"
        android:text="%"
        android:textAlignment="center"
        android:textColor="@color/transparent_backcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rvdatapostc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView39"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>