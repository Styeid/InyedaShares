        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/tvfundnamec"
        app:layout_constraintTop_toTopOf="@+id/tvfundnamec"
        tools:text="@string/fill_data_fundd" />

