    <ImageButton
        android:id="@+id/btnrefresha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="274dp"
        android:layout_marginTop="234dp"
        android:backgroundTint="#6D01579B"
        android:contentDescription="@string/clicker_refresh_data"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_refreshdata" />

    <ImageButton
        android:id="@+id/btnrefreshb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="234dp"
        android:backgroundTint="#6D01579B"
        android:contentDescription="@string/clicker_refresh_data"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnrefresha"
