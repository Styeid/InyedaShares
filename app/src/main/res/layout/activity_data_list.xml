        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/fill_data_funda" />

    <TextView
        android:id="@+id/tvfundnameb"
        android:layout_width="250dp"
