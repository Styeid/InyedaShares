    <TextView
        android:id="@+id/tvfundnamec"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/tvfundnameb"
        app:layout_constraintTop_toTopOf="@+id/tvfundnameb"
        tools:text="@string/fill_data_fundc" />

    <TextView
