        android:layout_marginStart="32dp"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/tvfundnamea"
        app:layout_constraintTop_toTopOf="@+id/tvfundnamea"
