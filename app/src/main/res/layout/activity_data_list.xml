        android:textAlignment="center"
        android:textColorHint="@color/transparent_backcolor"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/tvamountaxyz"
        app:layout_constraintHorizontal_bias="0.0"
